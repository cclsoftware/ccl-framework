# Doxyfile 1.8.7

@INCLUDE = {{ config.doxy_common_path }}/common.doxy

#---------------------------------------------------------------------------
# Project related configuration options
#---------------------------------------------------------------------------
PROJECT_NAME           = "Core"
PROJECT_NUMBER         =
PROJECT_BRIEF          = 
PROJECT_LOGO           =
OUTPUT_DIRECTORY       = ../doxyout/

#---------------------------------------------------------------------------
# Configuration options related to warning and progress messages
#---------------------------------------------------------------------------
WARN_LOGFILE           = ../doxyout/warnings.txt

#---------------------------------------------------------------------------
# Configuration options related to the input files
#---------------------------------------------------------------------------
INPUT                  = ../../../../core \
						 {{ config.doxy_common_path }}/coredocshared.h \
                         coredoc.h 
						 
EXCLUDE_PATTERNS	+=	*/zlib/*
EXCLUDE_PATTERNS	+=	*/pnglib/*
EXCLUDE_PATTERNS	+=	*/bonjour/*

#---------------------------------------------------------------------------
# Configuration options related to the preprocessor
#---------------------------------------------------------------------------
INCLUDE_PATH           = 
INCLUDE_FILE_PATTERNS  =

#---------------------------------------------------------------------------
# Configuration options related to external references
#---------------------------------------------------------------------------
TAGFILES               =
GENERATE_TAGFILE       =
ALLEXTERNALS           = NO
