		<Model.Class name="Frame" parent="Element">
			<Model.Documentation x:id="doc">
				<String x:id="brief" text="A frame is an area in a perspective that can be either empty or occupied by the view of a window class"/>
				<String x:id="detailed" text="The &quot;groups&quot; attribute is list of group names from window classes, separated by spaces. It specifies which window classes can appear in the frame.&#10;Complex arrangements can be build with horizontal or vertical frame groups, which will create a layout container for their contained frames."/>
				<String x:id="code" text="&lt;Perspective name=&quot;MyPerspective&quot;&gt;&#10;&#9;&lt;Frame options=&quot;vertical&quot;&gt;&#10;&#9;&#9;&lt;Frame name=&quot;MyFrame1&quot; groups=&quot;group1&quot;/&gt;&#10;&#9;&#9;&lt;Frame name=&quot;MyFrame2&quot; groups=&quot;group2 group3&quot; default=&quot;windowClassA&quot;/&gt;&#10;&#9;&lt;/Frame&gt;&#10;&lt;/Perspective&gt;"/>
				<String x:id="language" text="xml"/>
				<List x:id="links">
					<String text="Perspective"/>
				</List>
			</Model.Documentation>
			<List x:id="members">
				<Model.Member name="default" type="string" typeName="string">
					<Model.Documentation x:id="doc">
						<String x:id="brief" text="name of a window class that should initially appear in the frame"/>
					</Model.Documentation>
				</Model.Member>
				<Model.Member name="condition" type="string" typeName="string">
					<Model.Documentation x:id="doc">
						<String x:id="brief" text="an absolute path of a property that is evaluated to check if this frame can be used"/>
						<String x:id="detailed" text="The condition can be inverted by prepending it with &quot;not &quot;"/>
					</Model.Documentation>
				</Model.Member>
				<Model.Member name="decor" type="string" typeName="string">
					<Model.Documentation x:id="doc">
						<String x:id="brief" text="name of a decorating form that is wrapped around the form of the window class"/>
						<String x:id="detailed" text="The decor form should include the actual content as view &quot;Content&quot; from controller &quot;$frame&quot;. The decor form name is prepended with &quot;Workspace.&quot;"/>
					</Model.Documentation>
				</Model.Member>
				<Model.Member name="friend" type="string" typeName="string">
					<Model.Documentation x:id="doc">
						<String x:id="brief" text="name of a frame thats gets activated instead of this frame on mouse click"/>
					</Model.Documentation>
				</Model.Member>
				<Model.Member name="width" type="string" typeName="metric">
					<Model.Documentation x:id="doc">
						<String x:id="brief" text="initial width"/>
					</Model.Documentation>
				</Model.Member>
				<Model.Member name="height" type="string" typeName="metric">
					<Model.Documentation x:id="doc">
						<String x:id="brief" text="initial height"/>
					</Model.Documentation>
				</Model.Member>
				<Model.Member name="groups" type="string" typeName="string">
					<Model.Documentation x:id="doc">
						<String x:id="brief" text="space separated list of window class group names that may appear in this frame"/>
					</Model.Documentation>
				</Model.Member>
				<Model.Member name="options" type="string" typeName="enum"/>
			</List>
			<Attributes x:id="attributes">
				<Attribute id="Class:SchemaGroups" value="Frame.Children Perspective.Children"/>
				<Attribute id="Class:ChildGroup" value="Frame.Children"/>
				<Attribute id="Class:DocGroup" value="Workspace"/>
			</Attributes>
		</Model.Class>
