					</Model.Documentation>
				</Model.Member>
			</List>
			<Attributes x:id="attributes">
				<Attribute id="Class:SchemaGroups" value="ViewsAndStatements Frame.Children"/>
				<Attribute id="Class:ChildGroup" value="Frame"/>
				<Attribute id="Class:DocGroup" value="Workspace"/>
			</Attributes>
		</Model.Class>
