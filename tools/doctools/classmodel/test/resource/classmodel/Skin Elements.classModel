					</Model.Documentation>
				</Model.Member>
				<Model.Member name="vscroll.name" type="string" typeName="string">
					<Model.Documentation x:id="doc">
						<String x:id="brief" text="name of the vertical scroll parameter"/>
					</Model.Documentation>
				</Model.Member>
			</List>
			<Attributes x:id="attributes">
				<Attribute id="Class:ChildGroup" value="Target"/>
				<Attribute id="Class:DocGroup" value="Views"/>
			</Attributes>
		</Model.Class>
