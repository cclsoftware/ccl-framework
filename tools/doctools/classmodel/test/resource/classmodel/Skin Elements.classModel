			</Model.Documentation>
			<Attributes x:id="attributes">
				<Attribute id="Class:SchemaGroups" value="ViewsAndStatements"/>
				<Attribute id="Class:ChildGroup" value="ViewsAndStatements"/>
				<Attribute id="Class:DocGroup" value="General"/>
