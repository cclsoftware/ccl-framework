				<String x:id="brief" text="Selects one of multiple available alternative cases based on a property or variable value"/>
				<String x:id="code" text="&lt;switch property=&quot;myProperty&quot;&gt;&#10; &lt;case value=&quot;1&quot;&gt;&#10;  &lt;!-- ... --&gt;&#10; &lt;/case&gt;&#10; &lt;case value=&quot;0&quot;&gt;&#10;  &lt;!-- ... --&gt;&#10; &lt;/case&gt;&#10; &lt;default&gt;&#10;  &lt;!-- ... --&gt;&#10; &lt;/defautl&gt;&#10;&lt;/switch&gt;"/>
				<String x:id="language" text="xml"/>
