		<Model.Class name="Style" parent="Element">
			<Model.Documentation x:id="doc">
				<String x:id="brief" text="Visual Style definition"/>
				<String x:id="detailed" text="A Style defines the visual appearance of a view. It is collection of definitions for images, colors, metrics and fonts.&#10;It can be assigned to a view element via it&apos;s &quot;style&quot; attribute.&#10;Styles with the attribute &quot;appstyle&quot; can also be accessed directly by the application for specific purposes."/>
				<String x:id="code" text="&lt;Style name=&quot;MyStyle&quot;&gt;&#10;&#9;&lt;Font name=&quot;textfont&quot; face=&quot;Arial&quot; size=&quot;10&quot; style=&quot;bold&quot;/&gt;&#10;&#9;&lt;Color name=&quot;textcolor&quot; color=&quot;#666666&quot; /&gt;&#10;&#9;&lt;Image name=&quot;background&quot; image=&quot;SomeImage&quot;/&gt;&#10;&#9;&lt;Metric name=&quot;padding.left&quot; value=&quot;7&quot;/&gt;&#10;&#9;&lt;Align name=&quot;textalign&quot; align=&quot;left&quot;/&gt;&#10;&#9;&lt;Options name=&quot;someOption&quot; value=&quot;23&quot;/&gt;&#10;&#9;&lt;Options name=&quot;someOption&quot; type=&quot;Align.align&quot; options=&quot;left top&quot;/&gt;&#10;&lt;/Style&gt;"/>
				<String x:id="language" text="xml"/>
			</Model.Documentation>
			<List x:id="members">
				<Model.Member name="appstyle" type="string" typeName="bool">
					<Model.Documentation x:id="doc">
						<String x:id="brief" text="make style accessible from all skin scopes"/>
					</Model.Documentation>
				</Model.Member>
				<Model.Member name="inherit" type="string" typeName="string">
					<Model.Documentation x:id="doc">
						<String x:id="brief" text="inherit elements from this style"/>
					</Model.Documentation>
				</Model.Member>
				<Model.Member name="forecolor" type="string" typeName="color">
					<Model.Documentation x:id="doc">
						<String x:id="brief" text="color of foreground"/>
					</Model.Documentation>
				</Model.Member>
				<Model.Member name="backcolor" type="string" typeName="color">
					<Model.Documentation x:id="doc">
						<String x:id="brief" text="color of background"/>
					</Model.Documentation>
				</Model.Member>
				<Model.Member name="hilitecolor" type="string" typeName="color">
					<Model.Documentation x:id="doc">
						<String x:id="brief" text="color used for a hilite or seleciton state"/>
					</Model.Documentation>
				</Model.Member>
				<Model.Member name="textcolor" type="string" typeName="color">
					<Model.Documentation x:id="doc">
						<String x:id="brief" text="color used for drawing text"/>
					</Model.Documentation>
				</Model.Member>
