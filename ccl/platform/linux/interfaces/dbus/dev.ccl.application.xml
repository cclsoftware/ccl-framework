<!DOCTYPE node PUBLIC "-//freedesktop//DTD D-BUS Object Introspection 1.0//EN"
 "http://www.freedesktop.org/standards/dbus/1.0/introspect.dtd">

<node name="/dev/ccl/Application">
	<interface name="dev.ccl.Application">

		<property name="ApplicationID" type="s" access="read"/>

		<method name="ProcessCommandLine">
			<arg name="args" type="as" direction="in"/>
		</method>

		<method name="Activate">
			<arg name="tokenString" type="s" direction="in"/>
		</method>

	</interface>
</node>
