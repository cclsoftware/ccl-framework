			<ShapeColorMapping name="#F0EFF1"	color="@Main.VSLayoutViewFillColor"/>
		</ShapeImage>
		<ShapeImage name="Box.Colored" url="icons/vs2017/Box_16x.svg">
			<ShapeColorMapping name="#F6F6F6"	color="@Main.VSViewBoxOutlineColor"/>
			<ShapeColorMapping name="#F0EFF1"	color="@Main.VSViewBoxFillColor"/>
		</ShapeImage>
		<ShapeImage name="Table.Colored" url="icons/vs2017/Table_16x.svg">
			<ShapeColorMapping name="#f6f6f6"	color="@Main.VSLayoutViewOutlineColor"/>
			<ShapeColorMapping name="#F0EFF1"	color="@Main.VSLayoutViewFillColor"/>
		</ShapeImage>
